{
    "name": "actengage/roles",
    "description": "A simple role management package for Laravel.",
    "keywords": ["user", "permissions", "roles", "management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "actengage/sluggable": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\Roles\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Actengage\\Roles\\RolesServiceProvider"
            ],
            "aliases": {

            }
        }
    }
}
